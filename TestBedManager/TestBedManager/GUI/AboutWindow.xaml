<Window x:Class="TestBedManager.AboutWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="About Testbed Manager" Height="255" Width="400" MinHeight="255" MinWidth="400" WindowStyle="ToolWindow" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
	<Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
		<TextBlock x:Name="TextBlockAbout" HorizontalAlignment="Stretch" Margin="15" TextWrapping="Wrap" VerticalAlignment="Stretch" Background="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}" Padding="15"><Run Text="Testbed Manager is a tool designed to assist users" /><LineBreak /><Run Text="managing an array of computers by enabling them to " /><LineBreak /><Run Text="perform troubleshooting and recovery tasks remotely " /><LineBreak /><Run Text="and en masse." /><LineBreak /><Run /><LineBreak /><Run /><LineBreak /><Run Text="Author: Megan Kelley" /><LineBreak /><Run Text="Last update: 2013" /></TextBlock>
		<Grid Margin="328,160,10,10" />
	</Grid>
</Window>