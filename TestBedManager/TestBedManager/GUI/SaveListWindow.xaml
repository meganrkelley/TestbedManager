<Window x:Name="WindowSaveList" x:Class="TestBedManager.SaveListWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Save List As..." Height="81.203" Width="286.466" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
        <TextBox x:Name="TextBoxListName" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185" Text="Untitled List" Height="20" MaxLength="50" />
        <Button x:Name="ButtonSave" Content="Save" HorizontalAlignment="Left" Margin="200,10,0,0" VerticalAlignment="Top" Width="55" IsDefault="True" Click="ButtonSave_Click" />
    </Grid>
</Window>